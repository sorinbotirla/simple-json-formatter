{
  "manifest_version": 3,
  "name": "Simple JSON Formatter",
  "version": "1.0.1",
  "description": "Formats JSON pages. Shows raw content if not valid JSON.",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": []
}